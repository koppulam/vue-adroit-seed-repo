{
    "name": "General-code-repo",
    "version": "1.0.0",
    "description": "Starter Kit with HBS, Assemble, Webpack ",
    "scripts": {
        "clean": "rimraf www-root/",
        "lint": "eslint --ext .js --ext .jsx src/",
        "lint:fix": "eslint --fix . --ext .js --ext .jsx src/",
        "build": "node --max_old_space_size=8192 ./node_modules/webpack/bin/webpack.js --progress --env.dev",
        "build:global-comps": "node --max_old_space_size=8192 ./node_modules/webpack/bin/webpack.js --env.dev --env.globalComps --env.globalCompsCSS && gulp copyGlobalBundledCssFiles && gulp copyGlobalBundledJsFiles && gulp copyGlobalVendorJsFiles",
        "build:Ada": "npm run build && gulp ",
        "build:prod": "webpack -p --env.prod && gulp copyFiles && gulp copyChunkFiles",
        "build:prod-global-comps": "node --max_old_space_size=8192 ./node_modules/webpack/bin/webpack.js -p --env.prod --env.globalComps --env.globalCompsCSS && && gulp copyGlobalBundledCssFiles && gulp copyGlobalBundledJsFiles && gulp copyGlobalVendorJsFiles",
        "build:analyse": "npm run build -- --env.analyse",
        "build:watch": "webpack --watch --env.dev",
        "start": "node --max_old_space_size=8192 ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --env.dev",
        "start:fast": "node --max_old_space_size=8192 ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --env.dev --env.fast",
        "sass": "gulp sass && gulp sass:watch",
        "docgen": "node_modules/.bin/jsdoc --configure .jsdoc.json --verbose"
    },
    "engines": {
        "node": ">=8.3.0"
    },
    "license": "MIT",
    "devDependencies": {
        "assemble": "0.24.3",
        "assemble-webpack": "^3.1.1",
        "autoprefixer": "^8.6.5",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^7.1.5",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "blazy": "^1.8.2",
        "browser-sync": "^2.24.7",
        "browser-sync-webpack-plugin": "^2.2.2",
        "case-sensitive-paths-webpack-plugin": "^2.1.1",
        "copy-webpack-plugin": "^4.5.2",
        "css-loader": "^0.28.11",
        "cssnano": "^3.10.0",
        "csso-webpack-plugin": "^1.0.0-beta.12",
        "eslint": "^5.11.1",
