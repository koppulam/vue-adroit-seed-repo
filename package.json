        "postcss-loader": "^2.0.10",
        "postcss-pxtorem": "^4.0.1",
        "prettier": "^1.14.2",
        "rimraf": "^2.6.2",
        "run-sequence": "^1.2.2",
        "sass-loader": "^6.0.6",
        "speed-measure-webpack-plugin": "^1.2.3",
        "style-loader": "^0.19.1",
        "vue": "^2.6.10",
