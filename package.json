        "gulp-cssnano": "^2.1.3",
        "gulp-pixrem": "^1.0.0",
        "gulp-postcss": "^8.0.0",
        "gulp-rename": "^1.4.0",
        "gulp-sass": "^4.0.2",
        "gulp-sourcemaps": "^2.6.4",
        "handlebars": "^4.0.12",
        "handlebars-helper-repeat": "^1.0.0",
        "hc-sticky": "^2.2.1",
        "html-webpack-exclude-assets-plugin": "0.0.5",
        "html-webpack-harddisk-plugin": "^0.1.0",
        "html-webpack-plugin": "^2.30.1",
        "jsdoc": "^3.5.5",
